{
  "name": "notushelper",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "ts-node": {
    "esm": true
  },
  "prisma": {
    "seed": "bun run prisma/seed.ts"
  },
  "scripts": {
    "build": "bun build src/main.ts --target bun --outdir build --preload reflect-metadata",
    "build:changelog": "npx @discordx/changelog --src src",
    "dev": "bun run --watch ./src/main.ts --preload reflect-metadata",
    "start": "sleep 5 && bunx prisma migrate deploy && bun run src/main.ts",
    "format": "biome format src/ --write"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/pagination": "^3.4.0",
    "@prisma/client": "^5.13.0",
    "@t3-oss/env-core": "^0.10.1",
    "cache-manager": "^5.2.3",
    "clickup.ts": "^1.0.6",
    "dayjs": "^1.11.10",
    "discord.js": "^14.7.1",
    "discordx": "^11.7.0",
    "dotenv": "^16.0.3",
    "fuse.js": "^6.6.2",
    "parse-ms": "^3.0.0",
    "pretty-ms": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "stale-while-revalidate-cache": "^3.2.1",
    "tsyringe": "^4.7.0",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.1",
    "@types/node": "^18.13.0",
    "@types/uuid": "^9.0.0",
    "prisma": "^5.13.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": "18.16.0",
    "npm": "9.5.1"
  }
}