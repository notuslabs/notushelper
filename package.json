    "cache-manager": "^5.2.3",
    "clickup.ts": "^1.0.6",
    "dayjs": "^1.11.10",
    "discord.js": "^14.7.1",
    "discordx": "^11.7.0",
    "dotenv": "^16.0.3",
    "fuse.js": "^6.6.2",
    "parse-ms": "^3.0.0",
    "pretty-ms": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "stale-while-revalidate-cache": "^3.2.1",
    "tsyringe": "^4.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/uuid": "^9.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "prisma": "^4.16.1",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
